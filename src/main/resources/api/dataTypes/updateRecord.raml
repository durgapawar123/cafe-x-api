#%RAML 1.0 DataType
type: object
properties:
  firstName:
    type: string
    example: "pavan"
    required: false
  lastName:
    type: string
    example: "suryavanshi"
    required: false
  address:
    type: string
    example: "Hadapsar"
    required: false
  city:
    type: string
    example: "Pune"
    required: false
  phoneNumber:
    type: number
    example: 12436567890
    required: false
  custimerId:
    type: number
    example: 1234
    required: true